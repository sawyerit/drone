<?xml version="1.0" encoding="utf-8" ?>
<Products>
	<Product name="shopify">
		<!-- tattly.com -->
		<Rules>
			<Or>
				<Rule type="cookies" property="name" value="_shopify"/>
				<Rule type="text" property="html" value="var Shopify = Shopify" />
				<Rule type="metas" property="content" value="cdn.shopify.com"/>
				<And mincount="2">
					<Rule type="links" property="href" value="cdn.shopify.com"/>
					<Rule type="scripts" property="src" value="cdn.shopify.com"/>
				</And>
			</Or>
		</Rules>
	</Product>
	<Product name="volusion">
		<!-- www.saferwholesale.com -->
		<Rules>
			<Or>
				<Rule type="text" property="html" value="Ecommerce Web Site Design by &lt;a href=&quot;http://www.volusion.com&quot;" />
				<Rule type="scripts" property="src" value="a/j/javascripts.js" />
				<And mincount="2">
					<Rule type="images" property="src" value="volusion.com"/>
					<Rule type="scripts" property="src" value="/v/vspfiles/"/>
					<Rule type="links" property="href" value="cdn1.volusion.com" />
					<Rule type="text" property="html" value="http://www.volusion.com" />
				</And>
			</Or>
		</Rules>
	</Product>
	<Product name="bigcommerce">
		<!-- .com -->
		<Rules>
			<Or>
				<Rule type="text" property="html" value="Shopping Cart Software powered by BigCommerce." />
				<Rule type="text" property="html" value="&lt;a href=&quot;http://www.bigcommerce.com/ecommerce-solution-features.php&quot;&gt;Ecommerce Solution&lt;/a&gt; by BigCommerce"/>
				<And mincount="2">
					<Rule type="images" property="src" value="cdn1.bigcommerce.com"/>
					<Rule type="links" property="src" value="cdn1.bigcommerce.com"/>
					<Rule type="text" property="html" value="cdn1.bigcommerce.com" />
					<Rule type="text" property="html" value="cdn2.bigcommerce.com" />
				</And>
			</Or>
		</Rules>
	</Product>
