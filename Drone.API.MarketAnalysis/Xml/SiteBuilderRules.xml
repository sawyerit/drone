<?xml version="1.0" encoding="utf-8" ?>
<Products>
	<Product name="wix">
		<!-- courtneytidman.co.uk -->
		<Rules>
			<Or>
				<Rule type="urls" property="address" value="wix.com" />
				<Rule type="generators" property="content" value="wix" />
				<Rule type="metas" property="http-equiv" value="x-wix"  />
				<And mincount="2">
					<Rule type="atags" property="href" value="wix" />
					<Rule type="stylesheets" property="href" value="wix.com" />
					<Rule type="links" property="href" value="wix.com" />
					<Rule type="scripts" property="src" value="wix.com" />
				</And>
			</Or>
		</Rules>
	</Product>
	<Product name="weebly">
		<!-- mobiledjsampler.com/ -->
		<Rules>
			<Or>
				<Rule type="urls" property="address" value="weebly" />
				<Rule type="generators" property="content" value="weebly" />
				<Rule type="metas" property="content" value="weebly.com" />
				<And mincount="2">
					<Rule type="stylesheets" property="href" value="cdn1.editmysite.com" />
					<Rule type="scripts" property="src" value="cdn1.editmysite.com" />
					<Rule type="atags" property="href" value="cdn1.editmysite.com" />
					<Rule type="text" property="html" value="wsite-" />
					<Rule type="text" property="html" value="weebly-" />
				</And>
			</Or>
		</Rules>
	</Product>
	<Product name="webs">
		<!-- abmwindowcleaning.com -->
		<Rules>
			<Or>
				<Rule type="urls" property="address" value=".webs.com" />
				<Rule type="urls" property="address" value="freewebs.com" />
				<And mincount="2">
					<Rule type="stylesheets" property="href" value="static.websimages.com" />
					<Rule type="scripts" property="src" value="static.websimages.com" />
					<Rule type="text" property="html" value="weebly" />
				</And>
				<Rule type="cookies" property="name" value="fwww" />
			</Or>
		</Rules>
	</Product>
	<Product name="yola">
		<!-- zumbahamilton.ca/ -->
		<Rules>
			<Or>
				<Rule type="urls" property="address" value="yolasite.com" />
				<And mincount="1">
					<Rule type="text" property="html" value="analytics.yola" />
				</And>
			</Or>
		</Rules>
	</Product>
	<Product name="1and1">
		<!-- .com -->
		<Rules>
			<Or>
				<Rule type="atags" property="href" value="http://cms01.initial-website.com/app/cms/logout.php" ignoreAbsURI="true" />
				<Rule type="text" property="href" value="This website was created using &lt;a href=&quot;http://mybusiness.1and1.com&quot; target=&quot;_blank&quot;&gt;1&amp;1 MyBusiness Site&lt;/a&gt;" />
				<Rule type="text" property="html" value="window.jQuery_1and1" />
				<And mincount="2">
					<Rule type="scripts" property="src" value="initial-website.com" />
					<Rule type="images" property="src" value="initial-website.com" />
				</And>
			</Or>
		</Rules>
	</Product>
	<Product name="jimdo">
		<!-- bikewrappers.com -->
		<Rules>
				<And mincount="2">
					<Rule type="text" property="html" value="jimdoData" />
					<Rule type="links" property="href" value="jimdo.com" ignoreAbsURI="true" />
					<Rule type="atags" property="href" value="jimdo.com" />
					<Rule type="images" property="src" value="jimdo.com" ignoreAbsURI="true" />
				</And>
		</Rules>
	</Product>
